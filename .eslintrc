{
    "extends": "airbnb",
    "rules": {
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "jsx-a11y/anchor-is-valid": ["error", { "components": ["Link"], "specialLink": ["hrefLeft", "hrefRight"], "aspects": ["invalidHref", "preferButton"] }],
      "quotes": [
        "error",
        "double"
      ],
      "no-console": "off"
    },
    "parser": "babel-eslint",
    "env": {
      "browser": true
    }
}